SET SERVEROUTPUT ON;
DECLARE
  a NUMBER := 0; b NUMBER := 1; c NUMBER;
  n NUMBER := 10; -- how many terms
  i NUMBER := 2;
BEGIN
  DBMS_OUTPUT.PUT_LINE('Fibo terms:');
  DBMS_OUTPUT.PUT_LINE(a);
  DBMS_OUTPUT.PUT_LINE(b);
  WHILE i < n LOOP
    c := a + b;
    DBMS_OUTPUT.PUT_LINE(c);
    a := b;
    b := c;
    i := i + 1;
  END LOOP;
END;
/
